{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@/@types/*": [
        "./src/@types/*"
      ],
      "@/data/*": [
        "./src/data/*"
      ],
      "@/databases/*": [
        "./src/databases/*"
      ],
      "@/domain/*": [
        "./src/domain/*"
      ],
      "@/helpers/*": [
        "./src/helpers/*"
      ],
      "@/infra/*": [
        "./src/infra/*"
      ],
      "@/main/*": [
        "./src/main/*"
      ],
      "@/routes/*": [
        "src/main/routes/*"
      ],
      "@/services/*": [
        "./src/services/*"
      ],
      "@/ui/*": [
        "./src/ui/*"
      ],
    }
  }
}